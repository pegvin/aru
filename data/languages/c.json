{
	"name": "C",
	"keywords1": [
		"switch", "if", "while", "for", "break",
		"continue", "return", "else", "struct", "union",
		"typedef", "static", "enum", "class", "case"
	],
	"keywords2": [
		"int", "long", "double", "float",
		"char", "unsigned", "signed", "void"
	],
	"patterns": [
		{
			"pattern": "//.*",
			"type": "comment"
		},
		{
			"pattern": "(^#define*)|(^#include*)",
			"type": "keyword2"
		}
	],
	"extensions": [ ".c", ".h", ".inl" ],
	"sCommentStart": "//",
	"mCommentStart": "/*",
	"mCommentEnd": "*/"
}
